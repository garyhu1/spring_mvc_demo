<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="springHelloWorld" class="com.garyhu.impl.SpringHelloWorld"></bean>
    <bean id="strutsHelloWorld" class="com.garyhu.impl.StrutsHelloWorld"></bean>

    <bean id="helloWorldService" class="com.garyhu.service.HelloWorldService">
        <property name="helloWorld" ref="strutsHelloWorld"/>
    </bean>

    <bean id="user" class="com.garyhu.service.User">
        <property name="name" value="garyhu"/>
    </bean>
</beans>